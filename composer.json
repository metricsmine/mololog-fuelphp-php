{
    "name": "metricsmine/monolog-php",
    "version": "1.0.0",
    "description": "metricsmine integration for monolog. An abstract handler that sends messages to metricsmine",
    "keywords": ["metricsmine", "monolog", "logging", "error"],
    "type": "library",
    "homepage": "https://github.com/metricsmine/monolog-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo Martinez",
            "email": "info@metricsmine.com"
        }
    ],
    "require": {
        "php": "^5.6 | ^7.*",
        "monolog/monolog": "1.5.*",
        "metricsmine/client-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^1.5.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "support": {
        "email": "info@metricsmine.com",
        "issues": "https://github.com/metricsmine/monolog-php/issues"
    },
    "autoload": {
        "psr-4": {
            "metricsmine\\monolog-php\\": "src/"
        }
    }
}
